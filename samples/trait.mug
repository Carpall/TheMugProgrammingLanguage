type Shape(IGameObject) {
}

type Position2D {
  x: f128,
  y: f128

  pub fn from(x, y: f128): Position2D {
    new { x: x, y: y }
  }

  pub fn x(self: Position2D): f128 { self.x }
  pub fn y(self: Position2D): f128 { self.y }
}

type GameObject(IGameObject) = ( Shape )

trait IGameObject {
  pos: Position2D

  pub fn pos(self: IGameObject): Position2D { self.pos }
}

fn main() {
  const object: GameObject = Shape.from(pos)
  const p = object.pos() // called directly
}