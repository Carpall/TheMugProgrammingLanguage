const std = import!("std")

const Vector = std.coll.FixedArray

const Randomizer = std.int.Randomizer

fn main {
  let x = {
    let mut random = Randomizer(u32).create()

    let mut y: u32
    let mut r = Array(u32, 100).create()
    while y < 100 {
      r[y] = random.rand(0..y)
      y += 1
    }

    r
  }
}