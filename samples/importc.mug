const std = import!("std")

const readln = std.io.readln
const parse = std.num.parser.parse_num

const main = fn {
    let a = parse(i32, readln("a: "))
    let b = parse(i32, readln("b: "))
    let r = add(a, b)

    |importc: "c_add"|
    extern const add = fn(i32, i32): i32

    println!("add({}, {}): {}", a, b, r)
}